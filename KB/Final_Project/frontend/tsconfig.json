{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    //"target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    //"module": "esnext",
    //"moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    //"jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "moduleResolution": "node",
    "module": "esnext",
    "jsx": "preserve",
    "target": "esnext",
    "types": [
      "node",
      "react",
      "react-dom",
      "kakao.maps.d.ts"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
